{
  "kind": "collectionType",
  "collectionName": "practice_areas",
  "info": {
    "singularName": "practice-area",
    "pluralName": "practice-areas",
    "displayName": "Practice Area"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "Name"
    },
    "description": {
      "type": "text"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "content": {
      "type": "string"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "practice_area"
    },
    "neutrals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::neutral.neutral",
      "mappedBy": "practice_area"
    },
    "metadata": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "awards": {
      "type": "component",
      "component": "atoms.card",
      "repeatable": true
    }
  }
}
